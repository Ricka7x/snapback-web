#!/bin/bash
#
# Quick reference for Sparkle's generate_appcast path
# This is typically found in your Xcode DerivedData after building with Sparkle
#
# If you have Sparkle installed via package manager or locally:
# export SPARKLE_BIN="/path/to/sparkle/bin"
#
# Default location (Xcode DerivedData):
SPARKLE_BIN="/Users/ricka7x/Library/Developer/Xcode/DerivedData/Snapback-cgktvwlfyvupbvbgwoktbwlrhxnq/SourcePackages/artifacts/sparkle/Sparkle/bin"

# You can also set this as an environment variable
if [ -n "$SPARKLE_TOOLS_PATH" ]; then
  SPARKLE_BIN="$SPARKLE_TOOLS_PATH"
fi

export SPARKLE_BIN
